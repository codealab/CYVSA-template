<div class="container content">
      <div class="row">
            <div class="col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2 reg-page">
                  <%= form_for @service, :html => { :class => 'form-horizontal' } do |f| %>
                  <div class="panel panel-sea margin-bottom-40">
                    <div class="panel-heading">
                        <h3 class="panel-title"> Icono</h3>
                    </div>
                    <div class="panel-body">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                   
                                    <th>Icono</th>
                                   
                                    
                                    <th>Boton</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    
                                    <td> <div class="controls">
                              <i class= "color-blue" id="icono-show"></i>
                              <%= f.text_field :icon, :class => 'text_field', id: "service_icon",hidden: true %>
                        </div></td>
                                   
                                    <td class="col-md-3"><div class="btn btn-primary" data-toggle="modal" data-target="#icon-service">Seleciona un icono</div></td>
                                </tr>
                                
                            </tbody>
                        </table>
                    </div>                      
                </div>
                <h2> Servicios</h2>

                  <div class="control-group">
                        <%= f.label :Titulo_del_servicio, :class => 'control-label' %>
                        <div class="controls">
                              <%= f.text_field :title, :class => 'text_field' %>
                        </div>
                  </div>
                  <div class="field">
                        <%= f.label :DescripciÃ³n_del_servicio %><br>
                        <%= f.text_area :content, :class => 'wysihtml5' %>
                  </div>
                  
                  <% if params[:action] == "new" %>
                  
                  <div class="col-lg-3 text-right">
                        <%= f.submit "Crear", class: "btn btn-primary" %>
                  </div>
                  <% else %>
                  <div class="col-lg-3 text-right">
                        <%= f.submit "Guardar", class: "btn btn-primary" %>
                  </div>
                  <% end %>
                  <%= link_to "Cancelar", dashboard_path, class: 'btn btn-danger'%>
                  
            </div>
      </div>
</div>
<% end %>
