<div class="container content">
	<div class="row">
		<div class="col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2 reg-page">
			<%= form_for @year, :html => { :class => 'form-horizontal' } do |f| %>
			
			<div class="control-group">
				<%= f.label :year :class => 'control-label' %>
				<div class="controls">
					<%= f.text_field :year, :class => 'text_field' %>
				</div>
			</div>

			

			<div class="col-lg-3 text-right">
				<%= f.submit "Guardar",class: 'btn-u btn-brd btn-brd-hover btn-u-blue'%>
			</div>
		</div>
	</div>
</div>
<% end %>
